{
  "name": "ordercloud-angular-sdk",
  "version": "1.0.42-preview",
  "description": "Wrapper for OrderCloud-Javascript-SDK",
  "main": "src/index.js",
  "scripts": {
    "build": "npm run concat && npm run minify",
    "concat": "browserify ./src/index.js --outfile ./dist/ordercloud-angular-sdk.js",
    "minify": "uglifyjs --compress unused=false --mangle --output dist/ordercloud-angular-sdk.min.js -- dist/ordercloud-angular-sdk.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cobrien451/OrderCloud-Angular-SDK.git"
  },
  "keywords": [
    "SDK",
    "OrderCloud",
    "Angular",
    "API"
  ],
  "author": "Four51",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cobrien451/OrderCloud-Angular-SDK/issues"
  },
  "homepage": "https://github.com/cobrien451/OrderCloud-Angular-SDK#readme",
  "dependencies": {
    "angular": "1.6.3",
    "ordercloud-javascript-sdk": "1.0.42-preview"
  },
  "devDependencies": {
    "browserify": "14.1.0",
    "uglifyjs": "2.8.18"
  }
}
